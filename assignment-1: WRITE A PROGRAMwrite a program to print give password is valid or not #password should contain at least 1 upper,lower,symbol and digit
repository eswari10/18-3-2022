n=input()

l=0

u=0

s=0

d=0

for i in n:

      if(i.islower()):

              l+=1

      elif(i.isupper()):

              u+=1

      elif(i.isdigit()):

              d+=1

      else:

              s+=1

if l>0 and u>0 and d>0 and s>0:

      print("Valid Password")

else:

      print("Invalid Password. Please enter a strong password"
